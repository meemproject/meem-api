		"discord-interactions": "^3.3.0",
		"discord.js": "^14.8.0",
		"dotenv": "^16.0.2",
		"emoji-name-map": "^1.2.9",
		"ethers": "^5.7.1",
		"ethers-multisend": "^2.4.0",
		"express": "^4.18.1",
		"fs-extra": "^10.1.0",
