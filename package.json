		"sharp": "^0.31.0",
		"slug": "^8.0.0",
		"string-argv": "^0.3.1",
		"superagent": "^8.0.0",
		"supertest": "^6.2.4",
		"tslib": "^2.4.1",
		"twitter-api-sdk": "^1.2.1",
