		"@slack/web-api": "^6.8.1",
		"@tableland/sdk": "^3.1.5",
		"@typechain/ethers-v5": "^10.1.0",
		"alchemy-sdk": "^2.1.1",
		"auth0": "^2.42.0",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.20.0",
		"busboy": "^0.3.1",
